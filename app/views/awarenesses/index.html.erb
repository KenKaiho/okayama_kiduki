<p id="notice"><%= notice %></p>

<h1>Listing Awarenesses</h1>

<table>
  <thead>
    <tr>
      <th>Awareness</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @awarenesses.each do |awareness| %>
      <tr>
        <td><%= awareness.id %></td>
	<td><%= awareness.title %></td>
	<td><%= awareness.url %></td>
        <td><%= link_to 'Show', awareness %></td>
        <td><%= link_to 'Edit', edit_awareness_path(awareness) %></td>
        <td><%= link_to 'Destroy', awareness, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Awareness', new_awareness_path %>
